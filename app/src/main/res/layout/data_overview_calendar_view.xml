<?xml version="1.0" encoding="utf-8"?>
<com.example.brandon.habitlogger.ui.Widgets.CustomCalendar.OverviewCalendarView.CalendarView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:calendar_view="http://schemas.android.com/apk/res-auto"
    android:id="@+id/calendar_view"
    android:layout_width="match_parent"
    android:layout_height="390dp"

    android:layout_marginBottom="2dp"
    android:layout_marginLeft="4dp"
    android:layout_marginRight="4dp"
    android:layout_marginTop="2dp"

    android:background="@drawable/background_simple_rectangle"
    android:textSize="20sp"

    calendar_view:content_margin_top="12dp"
    calendar_view:date_circle_radius="19dp"
    calendar_view:date_circle_streak_color="@color/colorAccent"
    calendar_view:date_element_color="@color/calendarViewTitle"
    calendar_view:date_label_color="@color/calendarViewDateLabel"
    calendar_view:date_label_size="12sp"
    calendar_view:empty_date_color="@color/calendarViewEmpty"
    calendar_view:title_color="@color/calendarViewTitle"
    calendar_view:title_margin_top="6dp"/>